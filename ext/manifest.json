{

    "manifest_version": 2,
    "name": "ReFingerprint",
    "version": "0.1",

    "description": "WebExtension f√ºr ReFingerprint",

    "icons": {
        "48": "icons/logo.png"
    },

    "content_scripts": [{
        "matches": ["*://localhost/*"],
        "js": ["dist/bundle.js"]
    }],

    "background": {
        "scripts": ["dist/background.js"],
        "persistent": false
    },

    "permissions": [
        "cookies",
        "downloads",
        "downloads.shelf",
        "*://*.google.com/*"
    ]

}